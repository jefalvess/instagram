{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "scripts": {
    "ci-check": "yarn format:diff",
    "format:diff": "prettier --list-different \"src/**/*.{js,vue,scss}\"",
    "test:unit": "vue-cli-service test:unit",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "watch": "vue-cli-service build --mode development --watch",
    "nodemon": "nodemon instagram.js",
    "dev": "concurrently --kill-others \"npm:watch\" \"npm:nodemon\"",
    "start:prod": "vue-cli-service build --mode production && node instagram.js",
    "start:qa": "vue-cli-service build --mode test && node instagram.js",
    "start:dev": "vue-cli-service --mode development build && node instagram.js",
    "start": "node instagram.js"
  },
  "dependencies": {
    "@carbon/icons-vue": "^10.6.1",
    "@carbon/vue": "^2.21.0",
    "@cloudant/cloudant": "^4.5.1",
    "@vue/cli": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.4",
    "axios": "^0.21.1",
    "axios-timing": "^1.0.2",
    "carbon-components": "^10.26.0",
    "cheerio": "^1.0.0-rc.3",
    "child_process": "^1.0.2",
    "concurrently": "^6.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "instagram-web-api": "^2.2.2",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.4",
    "moment": "^2.29.1",
    "mongodb": "^4.0.1",
    "multer": "^1.4.3",
    "nodemon": "^2.0.12",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "socket.io": "^4.2.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.12",
    "vue-cookies": "^1.7.4",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.5.2",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^4.5.10",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-plugin-vue": "^7.4.1",
    "mocha": "^9.1.1",
    "node-sass": "^5.0.0",
    "sass-loader": "10.1.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.12"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "description": "This tutorial will guide you in creating a Vue app with the [Carbon Design System](https://www.carbondesignsystem.com/). Weâ€™ll teach you the ins and outs of using Carbon components, while introducing web development best practices along the way.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carbon-design-system/carbon-tutorial-vue.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carbon-design-system/carbon-tutorial-vue/issues"
  },
  "homepage": "https://github.com/carbon-design-system/carbon-tutorial-vue#readme",
  "main": "instagram.js",
  "keywords": []
}
